  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-register": "^6.23.0",
    "dedent": "^0.6.0",
    "electron": "1.6.0",
    "electron-mocha": "^3.3.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "temp": "^0.8.3"
  },
  "dependencies": {
    "ast-util": "^0.6.0",
    "leveldown": "^1.6.0",
    "levelup": "^1.3.3",
    "recast": "^0.11.22",
    "resolve": "^1.3.1"
  }
}
